#include <stdio.h>

int main() {
    char ch;
    int num;
    float fnum;
    double dnum;
    
    // Read input: char, int, float, double
    scanf("%c %d %f %lf", &ch, &num, &fnum, &dnum);
    
    // Print output: char, int, float (2 decimal places), double (6 decimal places)
    printf("%c %d %.2f %.6f\n", ch, num, fnum, dnum);
    
    return 0;
}
